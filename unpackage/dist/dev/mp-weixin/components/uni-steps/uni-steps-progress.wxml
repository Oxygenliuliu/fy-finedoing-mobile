<view class="uni-steps"><view class="{{['uni-steps-items','uni-steps-'+direction]}}"><block wx:for="{{options}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['uni-steps-item',(item.date=='')?'uni-steps-process':'',(item.date!='')?'uni-steps-finish':'']}}"><view class="uni-steps-item-title-container" style="{{'color:'+(item.date!=''?activeColor:'')+';'}}"><view class="uni-steps-item-title">{{item.proc_name}}</view><view class="uni-steps-item-desc">{{item.date}}</view></view><view class="uni-steps-item-circle-container"><block wx:if="{{item.date==''}}"><view class="uni-steps-item-circle" style="{{'background-color:'+(item.date!=''?activeColor:'')+';'}}"></view></block><block wx:else><uni-icons vue-id="{{'1-'+index}}" color="{{activeColor}}" type="checkbox-filled" size="14" bind:__l="__l"></uni-icons></block></view><block wx:if="{{index!==options.length-1}}"><view class="uni-steps-item-line" style="{{'background-color:'+(item.date!=''?activeColor:'')+';'}}"></view></block></view></block></view></view>